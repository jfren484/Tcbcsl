@model StateEditModel
@{
    Layout = "~/Areas/Admin/Views/Shared/_FormFieldLayout.cshtml";
}
@section Editable
{
    @Html.DropDownListFor(m => m.StateId, Model.ItemSelectList, new { @class = "form-control" })
    @Html.ValidationMessageFor(m => m.StateId)
}
@section Readonly
{
    @Html.HiddenFor(m => m.StateId)
    <p class="form-control-static">@Model.StateName</p>
}
