@model ScheduleGameRowModel<IScheduleGameRowDataModel>
<div class="row">
    <div class="col-xs-10 schedule-game-cell">@Html.EditorFor(m => m.LabelData)</div>
    <div class="col-xs-2 schedule-game-cell">
        @Html.TextBoxFor(m => m.Runs, new { style = "width: 100%", @type = "number" })
    </div>
</div>
<div class="row">@Html.ValidationMessageFor(m => m.Runs)</div>
