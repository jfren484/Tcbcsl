@model IEnumerable<NewsItemViewModel>
@{
    ViewBag.Title = "Home Page";
}

<h1>Twin Cities Baptist Church Softball League News</h1>

<div class="container">
    <div class="row">
        @foreach (var newsItem in Model)
        {
            <div class="col-md-4">
                <h2>@newsItem.StartDate - @newsItem.Subject</h2>
                <p>@newsItem.Content</p>
            </div>
        }
    </div>
</div>