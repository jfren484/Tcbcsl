@model LeagueIndividualStatisticsPageModel
@{
    const string title = "Statistics - League - Individual";
    ViewBag.Title = title + Model.Year.AsTitleSuffix();
}
@Html.Partial("_YearsPartial", Model.Year)
<h2>@title</h2>
<table id="statsTable" class="table table-bordered table-condensed table-striped stats-table">
    <thead>
        <tr class="info"></tr>
    </thead>
</table>
@section scripts
{
    <script>
        $(function () {
            statstable_RenderLeagueIndividualStats('@Model.Year.AsRouteParameter()');
        });
    </script>
}
