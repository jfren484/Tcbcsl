@model ScheduleModel
@{
    const string title = "Schedule";
    ViewBag.Title = $"{title} for {Model.Date.ToString(Consts.DateFormatDisplay)}";
}
<div class="modal calendar-modal">
    <div class="modal-dialog">
        <div class="modal-content calendar-content">
            <a asp-action="YearCalendar" asp-route-year="@Model.Date.Year" asp-route-activeDate="@Model.Date.ToString(Consts.DateFormat)"></a>
        </div>
    </div>
</div>
<div class="dropdown calendar-dropdown pull-right">
    <button class="btn btn-info dropdown-toggle" type="button" data-toggle="modal" data-target=".calendar-modal">
        @Model.Date.ToString(Consts.DateFormatDisplay)
        <span class="caret"></span>
    </button>
</div>
<h2>@title</h2>
@Html.DisplayFor(m => m.ConferenceModels)
